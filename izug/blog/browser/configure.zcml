<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="izug.blog">


    <!-- Resource directory -->
    <browser:resourceDirectory
        name="izug.blog.icons"
        directory="icons"
        /> 

   <browser:page
      for="*"
      name="blog_entry_view"
      class=".views.BlogEntryView"
      permission="zope.Public"
      template="blog_entry_view.pt"
      />  



</configure>
